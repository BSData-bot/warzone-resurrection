      <profiles/>
      <rules/>
      <infoLinks/>
      <modifiers/>
      <description>This squad uses the army rules of their army commander.</description>
    </rule>
    <rule id="55fc-36d2-10ca-d2dc" name="Bodyguard" hidden="false">
      <profiles/>
      <rules/>
      <infoLinks/>
      <modifiers/>
      <description>Hits can be transferred to this model using the guarded rule even if they are inflicted by a close combat attack or made by a model with Sniper.</description>
    </rule>
